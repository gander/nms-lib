{
    "name": "@gander/nms-lib",
    "description": "NMS Calc Library",
    "version": "1.0.1",
    "keywords": [
        "nomanssky",
        "No Man's Sky",
        "calculator"
    ],
    "author": "Gander",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/gander/nms-lib/issues"
    },
    "homepage": "https://github.com/gander/nms-lib",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build:esm": "tsc -p tsconfig.esm.json",
        "build": "bun run build:cjs && bun run build:esm",
        "clean": "rimraf dist",
        "prepack": "bun run clean && bun run build",
        "release": "release-it",
        "test": "vitest",
        "test-ui": "vitest --ui"
    },
    "devDependencies": {
        "@types/bun": "^1.1.14",
        "@types/node": "^22.10.2",
        "@vitest/ui": "^2.1.8",
        "release-it": "^17.11.0",
        "vitest": "^2.1.8"
    },
    "peerDependencies": {
        "typescript": "^5.5.0",
        "@vue/reactivity": "^3.5.0"
    }
}
